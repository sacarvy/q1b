---
import type { HTMLAttributes } from "astro/types";
import { cn } from "~/lib/utils";
export type Props = Omit<HTMLAttributes<"p">, "slot">;
const { class: className, ...attrs } = Astro.props;
---

<p class={cn("text-lg font-semibold dark:text-slate-300 text-slate-600", className)} {...attrs}>
    <slot />
</p>
