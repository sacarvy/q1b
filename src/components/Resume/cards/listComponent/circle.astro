---
const { type, x, y } = Astro.props;
---

{type==='single' ? <circle id="circle-2" cx={`${x}`} cy={`${y}`} r="6" fill="currentColor" class="text-slate-500"></circle> : (
            <circle id="circle-1" cx={`${x}`} cy={`${y}`} r="6" fill="currentColor" class="text-slate-500"></circle>
            <circle id="circle-1" cx={`${x}`} cy={`${y}`} r="11" fill="none" stroke="currentColor" stroke-width="2">       </circle>            
    )
}
