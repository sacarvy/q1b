<Layout title="Button Component">
    <main class="w-full max-w-3xl mt-16 mb-96">
        <h1 class="text-5xl text-slate-600 dark:text-slate-300 font-robotic">Dialog Panel</h1>
        <div class="flex absolute top-1/2 left-1/2 z-20 -translate-x-1/2 -translate-y-1/2">
            <div
                id="dialog-root"
                class="relative isolate"
            >

                <!-- Panel -->
                <div
                    id="dialog-panel"
                    class="dark:text-white text-slate-600 rounded-md bg-white dark:bg-slate-800 shadow-md border border-slate-200 dark:border-slate-600 p-4 flex flex-col gap-y-2"
                >
                    <div class="text-2xl sm:text-4xl">Payment successful</div>
                    <p>Lorem ipsum dolor sit amet consectetur adipisicing elit. Consequatur amet labore.</p>
                    <!-- Button -->
                    <Button
                        aria-controls="dialog-panel"
                        type="button"
                        class="gap-x-1"
                    >
                        <!-- heroicons:check -->    
                        <!-- <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" class="w-5 h-5">
                            <path fill-rule="evenodd" d="M16.704 4.153a.75.75 0 01.143 1.052l-8 10.5a.75.75 0 01-1.127.075l-4.5-4.5a.75.75 0 011.06-1.06l3.894 3.893 7.48-9.817a.75.75 0 011.05-.143z" clip-rule="evenodd" />
                        </svg> -->
                        Go to dashboard
                    </Button>
                </div>
            </div>
        </div>
    </main>
    <div id="dialog-overlay" class="inset-0 absolute bg-slate-300/50 dark:bg-slate-800/50 backdrop-blur-md backdrop-saturate-150 backdrop-contrast-125 shadow-inner shadow-white/20 z-10"></div>
</Layout>

